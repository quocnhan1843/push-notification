{
  "env": {"node": true},
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "no-var": 2,
    "no-console": 0,
    "comma-dangle": 0,
    "require-jsdoc": 0,
    "no-useless-escape": 0,
    "object-curly-spacing": ["error", "always"],
    "arrow-parens": ["error", "as-needed"],
    "max-len": ["error", { "ignoreStrings": true }]
  },
  "parserOptions": {
    "ecmaVersion": 6
  }
}
